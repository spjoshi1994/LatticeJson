			"command" : "python3 raw2kittivoc.py --dataset_dir /lattice/dataset/#datasetname/ --dataset_type kitti" ,
			"image_url": "latticesemi/ubuntu:latest"
		},
		"voc" : {
			"command" : "python3 raw2kittivoc.py --dataset_dir /lattice/dataset/#datasetname/ --dataset_type voc",
			"image_url": "latticesemi/ubuntu:latest"
		}
	}
	}
